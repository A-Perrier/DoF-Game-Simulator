{
  "expo": {
    "name": "DoFgamesimulator",
    "slug": "DoFgamesimulator",
    "version": "0.0.1",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.anthonyperrier.dofgamesimulator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
